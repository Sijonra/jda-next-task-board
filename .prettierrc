{
	"printWidth": 90,
	"useTabs": true,
	"semi": true,
	"tabWidth": 4,
	"singleQuote": true,
	"jsxSingleQuote": true,
	"bracketSpacing": true,
	"bracketSameLine": true,
	"quoteProps": "as-needed",
	"trailingComma": "es5",
	"arrowParents": "always",
	"importOrder": [
		"^classnames/.*$|.*module.scss$",
		"<THIRD_PARTY_MODULES>",
		"^.*components/.*$",
		"^.*public/.*(.jpg|.png)$",
		"^.*public/.*.svg$",
		"^.*utils/.*$",
		"^.*constants/.*$",
		"^.*interfaces/.*$",
		"^[./]"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true,
	"importOrderCaseInsensitive": true
}
